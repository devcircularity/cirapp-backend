{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/users(.*)",
        "dest": "/api/users$1"
      },
      {
        "src": "/api/tasks(.*)",
        "dest": "/api/tasks$1"
      },
      {
        "src": "/api/jobs(.*)",
        "dest": "/api/jobs$1"
      },
      {
        "src": "/api/reports(.*)",
        "dest": "/api/reports$1"
      },
      {
        "src": "/uploads/(.*)",
        "dest": "/uploads/$1"
      },
      {
        "src": "/api/chats(.*)",
        "dest": "/api/chats$1"
      },
      {
        "src": "/api/messages(.*)",
        "dest": "/api/messages$1"
      },
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ],
    "env": {
      "MONGODB_URI": "@mongodb_uri",
      "PORT": "@default"
    },
    "build": {
      "env": {
        "MONGODB_URI": "@mongodb_uri",
        "PORT": "@default",
        "GOOGLE_MAPS_API_KEY": "@google_maps_api_key",
        "CLOUDINARY_CLOUD_NAME": "@cloudinary_cloud_name",
        "CLOUDINARY_API_KEY": "@cloudinary_api_key",
        "CLOUDINARY_API_SECRET": "@cloudinary_api_secret"
      }
    }
  }
  