{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": [
            "jutdo-7b90f-firebase-adminsdk-kmksq-0d97f632ad.json"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/api/users(.*)",
        "dest": "/api/users$1"
      },
      {
        "src": "/api/tasks(.*)",
        "dest": "/api/tasks$1"
      },
      {
        "src": "/api/jobs(.*)",
        "dest": "/api/jobs$1"
      },
      {
        "src": "/api/reports(.*)",
        "dest": "/api/reports$1"
      },
      {
        "src": "/api/maps/key",
        "dest": "/api/maps/key"
      },
      {
        "src": "/api/chats(.*)",
        "dest": "/api/chats$1"
      },
      {
        "src": "/api/messages(.*)",
        "dest": "/api/messages$1"
      },
      {
        "src": "/uploads/(.*)",
        "dest": "/uploads/$1"
      }
    ],
    "env": {
        "MONGODB_URI": "@MONGODB_URI",
        "PORT": "@PORT",
        "GOOGLE_MAPS_API_KEY": "@GOOGLE_MAPS_API_KEY",
        "CLOUDINARY_CLOUD_NAME": "@CLOUDINARY_CLOUD_NAME",
        "CLOUDINARY_API_KEY": "@CLOUDINARY_API_KEY",
        "CLOUDINARY_API_SECRET": "@CLOUDINARY_API_SECRET"
      },
      "regions": ["sfo"]
    
  }
  